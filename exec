To load an executable program in a child processes exec system call.
//execDemo.c  FILE ONE
#include<stdio.h>
#include<stdlib.h>
#include<unistd.h>
int main()
{
		char *args[]={"./EXEC",NULL};
		execv(args[0],args);
	
		printf("Ending-----");
	
	return 0;
}
FILE TWO
//EXEC.c
#include<stdio.h>
int main()
{
	printf("I am EXEC.c called by execv() ");
}
/*
RUN METHOD:
gcc -o EXEC EXEC.c
gcc -o execDemo execDemo.c
./execDemo

*/
